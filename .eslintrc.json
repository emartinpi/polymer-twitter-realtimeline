{
  "extends": "eslint:recommended",
  "rules": {
    "no-console": 0,
    "lines-around-comment": [2, { "beforeLineComment": true }],
    "semi": [2, "always"],
    "brace-style": [1, "stroustrup"],
    "camelcase": [2, {
      "properties": "always"
    }],
    "comma-spacing": 2,
    "indent": [2, 2],
    "keyword-spacing": 2,
    "max-params": [1, { "max": 6 }],
    "newline-after-var": 1,
    "no-spaced-func": 2,
    "no-whitespace-before-property": 2,
    "quotes": [1, "single"],
    "comma-dangle": [2, "never"],
    "curly": 2
  },
  "env": {
    "browser": true,
    "node": true
  },
  "plugins": [],
  "globals": {
    "__dirname": false,
    "Polymer": false,
    "Promise": false,
    "process": false,
    "twttr": false,
    "io": false
  }
}
